<!-- <wxs module="filter" src="../../utils/filter.wxs"></wxs> -->
<view class="page__bd" bindtap="goDetail" wx:for="{{list}}" wx:key="index">
		<view class="weui-form-preview">
			<view class="weui-form-preview__hd">
				<view class="weui-form-preview__item">
					<label class="weui-form-preview__label">{{item.billNo}}</label>
					<!-- <em class="weui-form-preview__value">"{{filter.format(item.status)}}"</em> -->
				</view>
			</view>
			<view class="weui-form-preview__bd">
				<view class="weui-form-preview__item">
					<label class="weui-form-preview__label">申请人</label>
					<text class="weui-form-preview__value">{{item.applyUserid}}</text>
				</view>
				<view class="weui-form-preview__item">
					<label class="weui-form-preview__label">报废日期</label>
					<text class="weui-form-preview__value">{{item.destroyDate}}</text>
				</view>
				<view class="weui-form-preview__item">
					<label class="weui-form-preview__label">资产名称</label>
					<text class="weui-form-preview__value">{{item.goodsName}}</text>
				</view>
			</view>
		</view>

	</view>